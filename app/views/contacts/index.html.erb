<div class="col-sm-12">
  <%= form_tag import_contacts_path, multipart: true do %>
    <div class="col-sm-4">
      <%= file_field_tag :file %>
    </div>
    <div class="col-sm-4">
      <%= submit_tag "Import CSV" %>
    </div>
    <div class="col-sm-4">
      
      <%= link_to "Download sample CSV", "/contact.csv"%>
    </div>
  <% end %>
</div><br>


<br><br>
<table class="table table-bordered table-striped">
  <th>Id</th>
  <th>Name</th>
  <th>email</th>
  <th>company</th>
  <th>conference</th>
  <th>Status</th>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr >
        <td><%= contact.id %></td>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.company %></td>
        <p>
        <td><%= link_to contact.tag_list  %></td></p>
        <td><%= contact.status %></td>
      </tr>
    <% end %>

  </tbody>
</table>
<div align="center">
  <%= will_paginate @contacts%>
</div>